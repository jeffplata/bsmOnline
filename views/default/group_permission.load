       $("input[name$='"+n+"']").attr('checked',x);
    });

    $("input[name*='|']").on('change', function(e){
        x = $(this).is(':checked');
        alert($(this).attr('name'));
        n = 'object_' + $(this).attr('name').split('|')[1];
        // alert(n);
        $("input[id='"+n+"']").attr('checked',false);
        
    });
