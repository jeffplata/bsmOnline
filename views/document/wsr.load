        {{=grid}}
    </div>
{{else:}}
    {{=grid}}
{{pass}}

<script>
    {{=ASSIGNJS(varcon=var_container)}}
    $('#WSR_variety').on('change', function(e) {
        c = varcon[ $(this).find(':selected').val() ];
        $('#WSR_container').val(c).change();
    });
</script>